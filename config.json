{
  "s3_bucket": "dig-bio-index",
  "rds_instance": "dig-bio-index",
  "indexes": {
    "gene": {
      "comment": "gene by name",
      "s3_prefix": "genes/name",
      "table": "Gene",
      "schema": "name"
    },
    "genes": {
      "comment": "genes overlapping locus",
      "s3_prefix": "genes/region",
      "table": "Genes",
      "schema": "chromosome:start-end"
    },
    "global-enrichment": {
      "comment": "globally enriched tissues by trait",
      "s3_prefix": "global_enrichment/phenotype",
      "table": "GlobalEnrichment",
      "schema": "phenotype"
    },
    "regions": {
      "comment": "functional regions overlapping locus",
      "s3_prefix": "regions",
      "table": "AnnotatedRegions",
      "schema": "chromosome:start-end"
    },
    "associations": {
      "comment": "variant associations by trait overlapping locus",
      "s3_prefix": "associations/locus",
      "table": "Associations",
      "schema": "phenotype,chromosome:position"
    },
    "top-associations": {
      "comment": "most significant associations in locus",
      "s3_prefix": "associations/top",
      "table": "TopAssociations",
      "schema": "chromosome:position"
    },
    "phenotype-associations": {
      "comment": "genome-wide associations by trait",
      "s3_prefix": "associations/phenotype",
      "table": "PhenotypeAssociations",
      "schema": "phenotype"
    },
    "variant": {
      "comment": "lookup variant by ID",
      "s3_prefix": "variants",
      "table": "Variant",
      "schema": "varId"
    },
    "variants": {
      "comment": "variants by locus",
      "s3_prefix": "variants",
      "table": "Variants",
      "schema": "chromosome:position"
    }
  }
}
